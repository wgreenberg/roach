<table>
<thead>
    <tr>
    <th>Date</th>
    <th>Black</th>
    <th>White</th>
    <th>Game Type</th>
    <th>Outcome</th>
    </tr>
</thead>
<tbody>
{{#each games}}
    <tr>
    <td>{{this.outcome.time_started}}</td>
    <td><a href="/player/{{this.black.id}}">{{this.black.name}}</a></td>
    <td><a href="/player/{{this.white.id}}">{{this.white.name}}</a></td>
    <td>{{this.game_type}}</td>
    <td>{{this.outcome.status}}</td>
    <td><a href="/game/{{this.id}}">View</a></td>
    </tr>
{{/each}}
</tbody>
</table>
